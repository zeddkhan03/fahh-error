{
  "name": "fahh-error",
  "displayName": "FAHH! Error Sound",
  "description": "Plays the iconic fahh meme sound when errors appear in VS Code",
  "version": "0.0.2",
  "publisher": "zeddkhan",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeddkhan03/fahh-error"
  },
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onDidChangeDiagnostics"
  ],
  "main": "./out/extension.js",
  "icon": "icon.png",
  "contributes": {
    "configuration": {
      "title": "FAHH Error",
      "properties": {
        "fahhError.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable fahh sound when errors appear"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript": "^5.9.3",
    "eslint": "^9.39.2",
    "typescript-eslint": "^8.52.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
