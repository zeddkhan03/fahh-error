{
  "name": "fahh-error",
  "displayName": "FAHH! Error Sound",
  "description": "Plays the iconic FAHH meme sound whenever VS Code detects an error",
  "version": "0.1.6",
  "publisher": "MohdZaidKhan",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeddkhan03/fahh-error"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:fahhError.test"
  ],
  "main": "./extension.js",
  "icon": "icon1.png",
  "contributes": {
    "commands": [
      {
        "command": "fahhError.test",
        "title": "FAHH: Test Sound"
      }
    ],
    "configuration": {
      "title": "FAHH Error",
      "properties": {
        "fahhError.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable FAHH sound when VS Code detects errors"
        }
      }
    }
  }
}